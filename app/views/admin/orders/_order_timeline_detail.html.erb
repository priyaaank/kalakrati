<div class="panel panel-default">
  <div class="panel-heading">
    <i class="fa fa-clock-o fa-fw"></i> Order action history
  </div>
  <!-- /.panel-heading -->
  <div class="panel-body">
    <ul class="timeline">
      <li>
        <div class="timeline-badge success"><i class="fa fa-check"></i>
        </div>
        <div class="timeline-panel">
          <div class="timeline-heading">
            <h4 class="timeline-title">Delivered</h4>
            <p><small class="text-muted"><i class="fa fa-clock-o"></i> 11 hours ago via Twitter</small>
            </p>
          </div>
          <div class="timeline-body">
            <p>No additional info</p>
          </div>
        </div>
      </li>
      <li class="timeline-inverted">
        <div class="timeline-badge info"><i class="fa fa-truck"></i>
        </div>
        <div class="timeline-panel">
          <div class="timeline-heading">
            <h4 class="timeline-title">Shipped</h4>
            <p><small class="text-muted"><i class="fa fa-clock-o"></i> 11 hours ago via Twitter</small>
          </div>
          <div class="timeline-body">
            <p>No additional info</p>
          </div>
        </div>
      </li>
      <li>
        <div class="timeline-badge warning"><i class="fa fa-credit-card"></i>
        </div>
        <div class="timeline-panel">
          <div class="timeline-heading">
            <h4 class="timeline-title">Order placed</h4>
            <p><small class="text-muted"><i class="fa fa-clock-o"></i> 11 hours ago via Twitter</small>
          </div>
          <div class="timeline-body">
            <p>No additional info</p>
          </div>
        </div>
      </li>
    </ul>
  </div>
  <!-- /.panel-body -->
</div>
<!-- /.panel -->
</div>